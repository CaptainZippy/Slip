
(func ack (m:int n:int):int
    (cond
        ((eq_i? m 0:int)
            (add_i n 1:int))
        ((eq_i? n 0:int)
            (ack (sub_i m 1:int) 1:int))
        (true
            (ack (sub_i m 1:int) (ack m (sub_i n 1:int))))))

