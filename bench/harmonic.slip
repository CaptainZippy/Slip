
(func harmonic (n : int) : double (begin
    (var i 0:int)
    (var d 0:double)
    (while (lt? i n) (begin
        (set! i (add i 1))
        (set! d (addd d (divd 1.0 (dfromi i))))))
    d))

(func main ():int
    (begin
        (putd (harmonic 4))
        0))



